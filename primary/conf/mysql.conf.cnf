[mysqld]

default-storage-engine = InnoDB
bind-address = 0.0.0.0
server-id = 1
log-bin = /var/log/mysql/mysql-bin.log
sync-binlog = 1
gtid_mode = ON
log-slave-updates = ON
enforce-gtid-consistency = ON
master_info_repository = table
relay_log_info_repository = table
binlog_group_commit_sync_delay=300
slave_parallel_workers=2
slave_parallel_type='LOGICAL_CLOCK'
loose-group_replication_start_on_boot=OFF
loose-group_replication_group_name="bf1c5209-fd8a-49bb-8f34-ccf2811c1f27"
loose-group_replication_local_address="primary:33061"
loose-group_replication_group_seeds="primary:33061,secondary:33061,secondary2:33061"
loose-group_replication_ip_whitelist = "primary,secondary,secondary2"
loose-group_replication_single_primary_mode=ON
loose-group_replication_enforce_update_everywhere_checks=OFF
report_host="primary"
binlog_checksum = NONE
transaction_write_set_extraction = XXHASH64
loose-group_replication_bootstrap_group = OFF
slave-preserve-commit-order=1
loose-group_replication_ssl_mode = REQUIRED
loose-group_replication_recovery_use_ssl = 1
